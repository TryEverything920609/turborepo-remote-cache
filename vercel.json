{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/get/(.*)",
      "destination": "/src/plugins/remote-cache/routes/get-artifact.ts"
    },
    {
      "source": "/put/(.*)",
      "destination": "/src/plugins/remote-cache/routes/put-artifact.ts"
    }
  ]
}
